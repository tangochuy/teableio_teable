    "scroller": "^0.0.3",
    "sharedb": "4.1.0",
    "ts-key-enum": "^2.0.12",
    "ts-keycode-enum": "^1.0.6",
    "ts-mixer": "6.0.3",
    "zustand": "4.4.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@teable-group/eslint-config-bases": "workspace:^",
