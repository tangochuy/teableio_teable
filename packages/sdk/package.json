{
  "name": "@teable-group/sdk",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/sdk"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": "./dist"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc --build tsconfig.build.json",
    "build-react17jsx": "microbundle --tsconfig ./tsconfig.build.json --jsx jsx --jsxImportSource react --globals react/jsx-runtime=jsx --compress",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo ./node_modules/.cache",
    "dev": "microbundle watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/sdk.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "run-s test-unit",
    "test-unit": "jest --config jest.config.js --colors --logHeapUsage --silent",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix"
  },
  "dependencies": {
    "@belgattitude/http-exception": "^1.4.0",
