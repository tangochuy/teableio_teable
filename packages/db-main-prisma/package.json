{
  "name": "@teable-group/db-main-prisma",
  "version": "2.12.2",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/db-main-prisma"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prisma-studio": "dotenv-flow -- yarn prisma studio",
    "clean": "rimraf --no-glob ./dist ./build ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/db-main-prisma.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "run-s test-unit",
    "test-unit": "echo \"No unit tests yet\"",
    "test-e2e": "jest --config e2e/jest.config.js --maxWorkers=50% --color $@",
    "debug-test-e2e": "cross-env DEBUG=testcontainers jest --config e2e/jest.config.js --maxWorkers=50% --color $@",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "postinstall": "yarn db:sqlite:generate",
    "db:sqlite:schema": "cd ./prisma/sqlite && echo '{ \"PRISMA_PROVIDER\": \"sqlite\" }' | mustache - ../schema.mustache > schema.prisma && dotenv-flow -- yarn prisma migrate",
    "db:sqlite:generate": "yarn db:sqlite:schema && cd ./prisma/sqlite && yarn prisma generate",
    "db:sqlite:migrate": "yarn db:sqlite:schema && cd ./prisma/sqlite && dotenv-flow -- yarn prisma migrate",
    "db:pg:schema": "cd ./prisma/postgres && echo '{ \"PRISMA_PROVIDER\": \"postgresql\" }' | mustache - ../schema.mustache > schema.prisma",
    "db:pg:generate": "yarn db:pg:schema && cd ./prisma/postgres && yarn prisma generate",
    "db:pg:migrate": "yarn db:pg:schema && cd ./prisma/postgres && dotenv-flow -- yarn prisma migrate"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "nestjs-cls": "^3.5.1"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "dotenv-flow-cli": "1.0.0",
    "prisma": "5.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@soluble/dsn-parser": "1.6.0",
    "@teable-group/eslint-config-bases": "workspace:^",
    "@types/jest": "29.5.4",
    "@types/node": "18.11.7",
    "camelcase": "7.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.26.0",
    "execa": "5.1.1",
    "handlebars": "4.7.8",
    "is-port-reachable": "3.1.0",
    "jest": "29.7.0",
    "keyword-extractor": "0.0.23",
    "mustache": "4.2.0",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "prettier": "2.7.1",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "tsx": "3.11.0",
    "typescript": "4.8.4"
  }
}
