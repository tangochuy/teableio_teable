{
  "name": "@teable-group/db-main-prisma",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/db-main-prisma"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "dev": "tsc --watch",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/db-main-prisma.eslintcache",
    "prisma-generate": "prisma generate",
    "prisma-migrate": "dotenv-flow -p ../../apps/nextjs-app -- pnpm prisma migrate",
    "prisma-migrate-reset": "dotenv-flow -p ../../apps/nextjs-app -- pnpm prisma migrate reset",
    "prisma-db-push": "dotenv-flow -p ../../apps/nextjs-app -- pnpm prisma db push",
    "prisma-studio": "dotenv-flow -p ../../apps/nextjs-app -- pnpm prisma studio",
    "test": "run-s test-unit",
    "test-unit": "echo \"No unit tests yet\"",
    "test-e2e": "echo \"No e2e tests yet\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "@prisma/client": "^5.5.0",
    "@nestjs/common": "^10.0.0",
    "nestjs-cls": "^3.5.1"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "prisma": "5.5.2",
    "nanoid": "3.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@teable-group/eslint-config-bases": "workspace:^",
    "@types/jest": "29.5.7",
    "@types/node": "18.17.1",
    "camelcase": "8.0.0",
    "cross-env": "7.0.3",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.52.0",
    "handlebars": "4.7.8",
    "is-port-reachable": "3.1.0",
    "jest": "29.7.0",
    "mustache": "4.2.0",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "tsx": "3.14.0",
    "typescript": "5.2.2"
  }
}
