{
  "name": "@teable-group/db-main-prisma",
  "version": "2.12.2",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/db-main-prisma"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "dev": "tsc --watch",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/db-main-prisma.eslintcache",
    "postinstall": "prisma generate --schema=./prisma/sqlite/schema.prisma",
    "prisma-generate": "prisma generate",
    "prisma-migrate": "dotenv-flow -- yarn prisma migrate",
    "prisma-migrate-reset": "dotenv-flow -- yarn prisma migrate reset --schema=./prisma/sqlite/schema.prisma",
    "prisma-db-push": "dotenv-flow -- yarn prisma db push --schema=./prisma/sqlite/schema.prisma",
    "prisma-studio": "dotenv-flow -- yarn prisma studio",
    "test": "run-s test-unit",
    "test-unit": "echo \"No unit tests yet\"",
    "test-e2e": "echo \"No e2e tests yet\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "nestjs-cls": "^3.5.1"
  },
  "dependencies": {
    "@prisma/client": "5.4.1",
    "dotenv-flow-cli": "1.0.0",
    "nanoid": "3.3.6",
    "prisma": "5.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@teable-group/eslint-config-bases": "workspace:^",
    "@types/jest": "29.5.5",
    "@types/node": "18.11.7",
    "camelcase": "7.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.26.0",
    "execa": "5.1.1",
    "handlebars": "4.7.8",
    "is-port-reachable": "3.1.0",
    "jest": "29.7.0",
    "keyword-extractor": "0.0.23",
    "mustache": "4.2.0",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "prettier": "2.7.1",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "tsx": "3.11.0",
    "typescript": "4.8.4"
  }
}
