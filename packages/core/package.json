{
  "name": "@teable-group/core",
  "version": "1.1.1",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/core"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.js,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/core.eslintcache",
    "antlr4ts": "antlr4ts -visitor -no-listener src/formula/parser/*.g4",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js",
    "test-unit-watch": "vitest watch --ui",
    "fix-all-files": "eslint . --ext .ts,.js,.mjs,.cjs,.mts,.cts --fix"
  },
  "dependencies": {
    "antlr4ts": "0.5.0-alpha.4",
    "class-transformer": "0.5.1",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@size-limit/file": "8.1.0",
    "@teable-group/eslint-config-bases": "workspace:^",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.7",
    "@vitest/coverage-istanbul": "0.24.3",
    "@vitest/ui": "0.24.3",
    "antlr4ts-cli": "0.5.0-alpha.4",
    "cross-env": "7.0.3",
    "es-check": "7.0.1",
    "eslint": "8.26.0",
    "get-tsconfig": "4.2.0",
    "jest": "29.2.2",
    "rimraf": "3.0.2",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.3",
    "tslib": "2.4.0",
    "typescript": "4.8.4",
    "vite": "3.2.1",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.24.3"
  },
  "engines": {
    "node": ">=14.16"
  }
}
