{
  "name": "@teable-group/core",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "homepage": "https://github.com/teable-group/teable",
  "repository": {
    "type": "git",
    "url": "https://github.com/teable-group/teable",
    "directory": "packages/core"
  },
  "author": {
    "name": "tea artist",
    "url": "https://github.com/tea-artist"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.js,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/core.eslintcache",
    "antlr4ts": "antlr4ts -visitor -no-listener src/formula/parser/*.g4",
    "antlr4ts-tql": "antlr4ts -visitor -no-listener src/query/parser/*.g4",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "run test-unit",
    "test-unit": "jest --config jest.config.js --colors --logHeapUsage --silent",
    "fix-all-files": "eslint . --ext .ts,.js,.mjs,.cjs,.mts,.cts --fix"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "6.2.0",
    "@soluble/dsn-parser": "1.9.2",
    "antlr4ts": "0.5.0-alpha.4",
    "class-transformer": "0.5.1",
    "dayjs": "1.11.10",
    "lodash": "4.17.21",
    "nanoid": "3.3.6",
    "reflect-metadata": "0.1.13",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@size-limit/file": "9.0.0",
    "@testing-library/jest-dom": "6.1.4",
    "@teable-group/eslint-config-bases": "workspace:^",
    "@types/jest": "29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/node": "18.17.1",
    "antlr4ts-cli": "0.5.0-alpha.4",
    "cross-env": "7.0.3",
    "es-check": "7.1.1",
    "eslint": "8.51.0",
    "get-tsconfig": "4.7.2",
    "jest": "29.7.0",
    "rimraf": "5.0.5",
    "size-limit": "9.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
